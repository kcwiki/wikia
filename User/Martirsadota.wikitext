function opinions(item) {
    var isGoodMood = Math.random() > 0.85;
    if (typeof(item.name) != "string") {
        throw new TypeError("I have no opinions about that");
    } else if ((["Kongou","Hibiki","Inazuma","Shimakaze","Kaga"].indexOf(item.name) >= 0) && !isGoodMood) {
        throw new OverratedException(item.name + " is an overrated character");
    } else if (["Fubuki","Houshou","Akatsuki","Ikazuchi"].indexOf(item.name) >= 0) {
        console.log(item.name + " a good, " + item.name + " a very good");
    } else {
        console.warn(item.name + " is just fine");
    }
}

function OverratedException(e) {
    this.message = e;
    this.name = "OverratedException";
}
